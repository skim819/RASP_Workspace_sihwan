DAC06 = [ linspace(2.5,2.5,100) ];
DAC07 = [ linspace(2.5,2.5,100) ];

DAC01 = [ linspace(2.5,2.5,100) ];
DAC08 = [ linspace(2.5,2.5,100) ];
